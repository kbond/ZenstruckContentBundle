{
    "name": "zenstruck/content-bundle",
    "description": "Simple CMS for Symfony2 using Doctrine2 Class Table Inheritance",
    "keywords": ["content management"],
    "homepage": "http://zenstruck.com/project/ZenstruckContentBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com"
        }
    ],
    "require": {
        "symfony/symfony"           : "~2.2|~3.0",
        "doctrine/orm"              : "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle"  : "~1.0"
    },
    "require-dev": {
        "dpn/xml-sitemap-bundle"    : "~1.0"
    },
    "suggest": {
        "dpn/xml-sitemap-bundle"    : "For generating sitemaps"
    },
    "autoload": {
        "psr-0": { "Zenstruck\\Bundle\\ContentBundle": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "target-dir": "Zenstruck/Bundle/ContentBundle"
}
